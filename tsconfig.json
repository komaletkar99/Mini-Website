{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs", // Use CommonJS module system
      "outDir": "./dist",
      "rootDir": "./",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true
    },
    "include": [
      "backend/**/*.ts",
      "models/**/*.ts"
    ],
    "exclude": ["node_modules"]
  }
  